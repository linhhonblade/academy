<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="category_courses" model="product.public.category">
            <field name="name">Courses</field>
            <field name="parent_id"/>
        </record>
        <record id="course_name_sequence" model="ir.sequence">
            <field name="name">course.sequence</field>
            <field name="code">product.template</field>
            <field name="active">TRUE</field>
            <field name="prefix">COUR/%(year)s/</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>